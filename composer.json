{
    "name": "hiblaphp/sql",
    "description": "SQL client contracts for Hibla sql database libraries",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "hiblaphp/promise": "*"
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "tests/"
        }
    },
     "require-dev": {
        "phpstan/phpstan": "^2.0",
        "phpstan/phpstan-strict-rules": "^2.0",
        "laravel/pint": "^1.0",
        "pestphp/pest": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "Hibla\\Sql\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
